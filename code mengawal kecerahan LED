int led = 9;   

void setup() {
pinMode(led,OUTPUT);
}

void loop() {
dim();
light();
}

void dim() {
analogWrite(led,255);
delay(1000);
analogWrite(led,150);
delay(1000);
analogWrite(led,50);
delay(1000);
analogWrite(led,10);
delay(1000);
analogWrite(led,1);
delay(1000);
}



void light() {
analogWrite(led,1);
delay(1000);
analogWrite(led,10);
delay(1000);
analogWrite(led,50);
delay(1000);
analogWrite(led,150);
delay(1000);
analogWrite(led,255);
delay(1000);
}
